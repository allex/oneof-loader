{
  "name": "oneof-loader",
  "version": "1.0.1",
  "description": "Oneof loader for webpack",
  "main": "lib/index.js",
  "repository": "allex/oneof-loader",
  "keywords": [
    "webpack",
    "loader",
    "oneof"
  ],
  "scripts": {
    "precommit": "yarn lint && yarn test",
    "lint": "eslint src/* --fix",
    "test": "jest --coverage --passWithNoTests",
    "build": "yarn lint && rb -c",
    "prepare": "yarn build"
  },
  "author": "Allex Wang <allex.wxn@gmail.com>",
  "dependencies": {
    "loader-utils": "^1 || ^2"
  },
  "devDependencies": {
    "@babel/core": "^7.6.2",
    "@babel/preset-env": "^7.6.2",
    "@babel/runtime": "^7.6.2",
    "@types/node": "^13.1.8",
    "babel-core": "bridge",
    "babel-jest": "^23.6.0",
    "eslint": "^5.15.3",
    "eslint-config-airbnb-base": "~14.0.0",
    "eslint-config-fdio": "next",
    "eslint-import-resolver-typescript": "^2.0.0",
    "eslint-plugin-import": "^2.20.0",
    "husky": "^1.1.2",
    "jest": "^23.6.0"
  },
  "peerDependencies": {
    "webpack": "^4 || ^5"
  },
  "license": "MIT",
  "files": [
    "lib/"
  ],
  "jest": {
    "testEnvironment": "node",
    "coverageReporters": [
      "json",
      "lcovonly",
      "text",
      "clover"
    ],
    "modulePaths": [
      "src"
    ],
    "testMatch": [
      "**/?(*-)+(spec|test).?(m)js"
    ],
    "roots": [
      "test"
    ],
    "moduleFileExtensions": [
      "js"
    ],
    "moduleDirectories": [
      "node_modules"
    ],
    "transform": {
      "\\.js$": "babel-jest"
    }
  }
}
